{
  "extends": "airbnb",
  "plugins": [
    "react",
    "flowtype",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "jest": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "react/jsx-filename-extension": 0,
    "global-require": 0,
    "jsx-a11y/img-has-alt": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "react/prop-types": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0
  },
  "globals": {
    "__DEV__": true
  }
}
